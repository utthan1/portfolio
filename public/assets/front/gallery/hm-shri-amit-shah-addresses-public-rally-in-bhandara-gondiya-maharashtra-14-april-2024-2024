    <script type="text/javascript" src="https://amitshah.co.in/js/custom.js"></script>
    <script type="text/javascript" src="https://amitshah.co.in/js/frontend/custom.js?v=2"></script>


    
    <script>
        
$(document).ready(function(){

    let $likes_count = '0';


    $(document).on('click', '.like_post', function(e){
        e.preventDefault();

        $url = 'https://amitshah.co.in/api/v1/posts/54830/like';
        $element = $(this);

        if($element.find('i').hasClass('active')){
            $method = 'delete';
        }else{
            $method = 'post';
        }

        $.ajax({
            type: $method,
            url: $url,
            processData: false,
            dataType: "json",
            contentType: 'application/json',
            complete: function (data) {
                hideLoader();
            },
            success: function( $res) {
                if($method == 'delete'){
                    $('.like_post i').removeClass('active');
                    $likes_count = parseInt($likes_count)-1;
                }else{
                    $('.like_post i').addClass('active');
                    $likes_count = parseInt($likes_count)+1;
                }
                $('.like_post span').text(formatNumber($likes_count));
            },
            error: function(xhr, textStatus, errorMessage) {
                try {
                    $res= $.parseJSON(xhr.responseText);
                    if($res.message.toLocaleLowerCase().indexOf("unauthenticated")!=-1){
                        showSweetalert('Please login to perform this action. <br><a href="'+site_url+'/login" class="err_pp_login">LOGIN</a>',0);
                    }else{
                        showToastr($res.message,0);
                    }
                } catch(e) {
                    showToastr('Request could not be completed',0);
                }
            }             
        });
    }); //end

    if($('.common_slider1').length){
        $('.common_slider1').owlCarousel({
          loop: false,
          margin:0,
          nav: true,
          dots:false,
          autoplay:false,
          rewind: true,
          autoplaySpeed: 1600,
          items: 1,
          responsiveClass: true,
        /*  responsive: {
            1000: { items: 4,  },
            800: {  items: 3,  },
            600: {  items: 2,  },
            0: {  items: 1,  }
          }*/
        });
