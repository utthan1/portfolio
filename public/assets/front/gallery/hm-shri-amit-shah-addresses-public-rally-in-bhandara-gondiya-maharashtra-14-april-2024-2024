    $(document).on('click', '.like_comment', function(e){
        e.preventDefault();

        $comment_id = $(this).data('id');
        $count = $(this).data('count');

        $url = 'https://amitshah.co.in/api/v1/comments/:id/like';
        $url = $url.replace(':id', $comment_id);
                                
        $element = $(this);

        if($element.find('i').hasClass('active')){
            $method = 'delete';
        }else{
            $method = 'post';
        }

        $.ajax({
            type: $method,
            url: $url,
            processData: false,
            dataType: "json",
            contentType: 'application/json',
            complete: function (data) {
                hideLoader();
            },
            success: function( $res) {
                if($method == 'delete'){
                    $element.find('i').removeClass('active');
                    $count = parseInt($count)-1;
                }else{
                    $element.find('i').addClass('active');
                    $count = parseInt($count)+1;
                }


                $element.data('count',$count);
                $element.find('span').text(formatNumber($count));
            },
            error: function(xhr, textStatus, errorMessage) {
                try {
                    $res= $.parseJSON(xhr.responseText);
                    if($res.message.toLocaleLowerCase().indexOf("unauthenticated")!=-1){
                        showSweetalert('Please login to perform this action. <br><a href="'+site_url+'/login" class="err_pp_login">LOGIN</a>',0);
                    }else{
                        showToastr($res.message,0);
                    }
                } catch(e) {
                    showToastr('Request could not be completed',0);
                }
            }             
        });

    }); //end

    
    $('#commentForm').validate({
            ignore: [],
            rules: {
                comment : {required: true},
                // 'g-recaptcha-response' : {required: true},
            },

            errorElement: 'span',
            errorPlacement: function (error, element) {
              error.addClass('invalid-feedback');
              element.closest('.form-group').append(error);
            },
            highlight: function (element, errorClass, validClass) {
              $(element).addClass('is-invalid');
            },
            unhighlight: function (element, errorClass, validClass) {
              $(element).removeClass('is-invalid');
            },

            submitHandler: function(form) {
                var object = getFormDataObject(form);

                var json = JSON.stringify(object);

                $button = $('#comment_submit_form');
                showLoader();
                
                $.ajax({
                    type: $(form).attr('method'),
                    url: $(form).attr('action'),
                    data: json,
                    processData: false,
                    dataType: "json",
                    contentType: 'application/json',
                    complete: function (data) {
                        // grecaptcha.reset(contact_captcha);
                        hideLoader();
                    },
                    success: function( $res) {
                        $('#commentForm textarea').val('');
                        showSweetalert('Your comment will be published after moderation.',1);
                    },
                    error: function(xhr, textStatus, errorMessage) {
                        handleErrorMessage(xhr, textStatus,2);
                    }             
                });
            }
        }); //end

 });

</script>
<style type="text/css">
.gallery_slider .owl-item img {
  height: auto;
}
</style>

    

    <script type="text/javascript">
        (function(){
            var cookies;
            function readCookie(name,c,C,i){
                if(cookies){ return cookies[name]; }
                c = document.cookie.split('; ');
                cookies = {};

                for(i=c.length-1; i>=0; i--){
                   C = c[i].split('=');
                   cookies[C[0]] = C[1];
                }
                return cookies[name];
            }
            window.readCookie = readCookie;
        })();

        
        if(! readCookie('mb_loc')){
            if (window.navigator.geolocation) {
                var failure, success;
                success = function(position) {
                    console.log(position);
                    
                    var object = {
                        'lat' : position.coords.latitude,
                        'lon' : position.coords.longitude,
                    };
                    $.ajax({
                        type: 'POST',
                        url: 'https://amitshah.co.in/api/v1/set-location',
                        data: JSON.stringify(object),
                        contentType: 'application/json',
                        success: function( $res) {
                            console.log($res);
                        },
                        error: function(xhr, textStatus, errorMessage) {
                            try {
                                $res= $.parseJSON(xhr.responseText);
                                console.log($res);
                            } catch(e) {
                                console.log('error');
                            }
                        }             
                    });
                };
                failure = function(message) {
                    console.log('Cannot retrieve location!');
                };
                navigator.geolocation.getCurrentPosition(success, failure, {
                  maximumAge: Infinity,
                  timeout: 5000
                });
            }
        }

        $('.lang_switcher a').on('click',function(e){
            e.preventDefault();
            // $val = $('.lang_switcher').val();
            $val = $(this).data('lang');
            $locale = 'en';
            $current = 'https://amitshah.co.in/gallery/hm-shri-amit-shah-addresses-public-rally-in-bhandara-gondiya-maharashtra-14-april-2024-2024';

            if($val == 'en' && $locale != 'en'){
                  if($current.indexOf("/"+$locale+"/") != -1){
                      $new_url = $current.replace("/"+$locale+"/", "/");
                  }
                  else if($current.indexOf("/"+$locale) != -1){
                      $new_url = $current.replace("/"+$locale, "/");
                  }
            }
            else if($locale == 'en' && $val != 'en'){
                $new_url = $current.replace(site_url, site_url+"/"+$val);
            }
            else {
                return false;
                // $new_url = $current;
            }

            window.location.href = $new_url;
            return false;
        });
        
    </script>

