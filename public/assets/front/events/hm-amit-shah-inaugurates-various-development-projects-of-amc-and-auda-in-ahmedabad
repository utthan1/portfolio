<script>
        
$(document).ready(function(){
    document.cookie = 'cookie1=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/'

    $('.btn09_s2').click(function(){
        $.fancybox.close();
    });

    $('.register_button').click(function(){
        document.cookie = 'event_redirect='+window.location.href+'; expires=Sun, 1 Jan 2023 00:00:00 UTC; path=/'

        window.location.href = $(this).data('url');
        return false;
    });

    $(document).on('click', '#confirm_register', function(e){
        e.preventDefault();

        $ev_id = $(this).data('id');

        $url = 'https://amitshah.co.in/api/v1/events/:id/subscribe';
        $url = $url.replace(':id', $ev_id);
                                
        $element = $(this);

        $.ajax({
            type: 'post',
            url: $url,
            processData: false,
            dataType: "json",
            contentType: 'application/json',
            complete: function (data) {
                hideLoader();
            },
            success: function( $res) {
                $('.registerDiv').html('<p class="subscribed">Subscribed</p>')
                $.fancybox.close();
                showToastr($res.message);
            },
            error: function(xhr, textStatus, errorMessage) {
                try {
                    $res= $.parseJSON(xhr.responseText);
                    showToastr($res.message,0);
                } catch(e) {
                    showToastr('Request could not be completed',0);
                }
            }             
        });

    }); //end

     

});

</script>

    

    <script type="text/javascript">
        (function(){
            var cookies;
            function readCookie(name,c,C,i){
                if(cookies){ return cookies[name]; }
                c = document.cookie.split('; ');
                cookies = {};

                for(i=c.length-1; i>=0; i--){
                   C = c[i].split('=');
                   cookies[C[0]] = C[1];
                }
                return cookies[name];
            }
            window.readCookie = readCookie;
        })();

        
        if(! readCookie('mb_loc')){
            if (window.navigator.geolocation) {
                var failure, success;
                success = function(position) {
                    console.log(position);
                    
                    var object = {
                        'lat' : position.coords.latitude,
                        'lon' : position.coords.longitude,
                    };
                    $.ajax({
                        type: 'POST',
                        url: 'https://amitshah.co.in/api/v1/set-location',
                        data: JSON.stringify(object),
                        contentType: 'application/json',
                        success: function( $res) {
                            console.log($res);
                        },
                        error: function(xhr, textStatus, errorMessage) {
                            try {
                                $res= $.parseJSON(xhr.responseText);
                                console.log($res);
                            } catch(e) {
                                console.log('error');
                            }
                        }             
                    });
                };
                failure = function(message) {
                    console.log('Cannot retrieve location!');
                };
                navigator.geolocation.getCurrentPosition(success, failure, {
                  maximumAge: Infinity,
                  timeout: 5000
                });
            }
        }

        $('.lang_switcher a').on('click',function(e){
            e.preventDefault();
            // $val = $('.lang_switcher').val();
            $val = $(this).data('lang');
            $locale = 'en';
            $current = 'https://amitshah.co.in/events/hm-amit-shah-inaugurates-various-development-projects-of-amc-and-auda-in-ahmedabad';

            if($val == 'en' && $locale != 'en'){
                  if($current.indexOf("/"+$locale+"/") != -1){
                      $new_url = $current.replace("/"+$locale+"/", "/");
                  }
                  else if($current.indexOf("/"+$locale) != -1){
                      $new_url = $current.replace("/"+$locale, "/");
                  }
            }
            else if($locale == 'en' && $val != 'en'){
                $new_url = $current.replace(site_url, site_url+"/"+$val);
            }
            else {
                return false;
                // $new_url = $current;
            }

            window.location.href = $new_url;
            return false;
        });
        
    </script>

